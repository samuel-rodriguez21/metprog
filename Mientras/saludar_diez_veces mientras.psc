Proceso saludar_diez_veces
	Escribir "Algoritmo que realiza un saludo 10 veces";
	Escribir "Autor: Josue Samuel Loyola Rodriguez";
	Escribir "____________________________________";
	
	Definir cont Como Entero;
	cont <- 1;
	Mientras cont <= 10 Hacer
		Escribir cont, ".- Hola a todos :)";
		cont <- cont + 1;
	FinMientras
FinProceso
